<odoo>
    <data>

        <record id="model_ubig_pendaftaran" model="ir.model">
            <field name="name">ubig.pendaftaran</field>
            <field name="model">ubig.pendaftaran</field>
        </record>

        <record id="ir_cron_hapus_pendaftaran" model="ir.cron">
            <field name="name">Hapus Pendaftaran Kadaluarsa</field>
            <field name="model_id" ref="model_ubig_pendaftaran" />
            <field name="state">code</field>
            <field name="code">model.hapus_pendaftaran_kadaluarsa()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="True" />
        </record>
    </data>
</odoo>